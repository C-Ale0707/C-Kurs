function mex_v1_all
% compile matrixmath and fir_filter_v1 on the fly
% and link to the respective object files

    mex_command = [ 'mex ',...
        '-v ',...
        '-Imatrixmath ',...
        '-Ifir_filter_v1 ',...
        'iir_sfunction.c ', ...
        'matrixmath/matrixmath.c ', ...
        'iir_filter_v1/fir_filter_v1.c'...
        ]
	eval(mex_command);

end
